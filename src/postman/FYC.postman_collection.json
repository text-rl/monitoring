								"    method: 'POST',",
								"    header: {",
								"        'Content-Type': 'application/json',",
								"    },",
								"    body: {",
								"        mode: 'raw',",
								"        raw: JSON.stringify({ email, password})",
								"    }",
								"}",
								"",
								"pm.sendRequest(authReq,(err, res) => {",
								"    console.log(res.json());",
								"    pm.environment.set(\"token\", res.json().token)",
								"    console.log(err);",
								"});"
